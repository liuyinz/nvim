{
  // suggest
  "suggest.maxPreviewWidth": 50,
  "suggest.enablePreview": false,
  "suggest.detailMaxLength": 30,
  "suggest.detailField": "preview", // ["abbr","menu","preview"]
  "suggest.autoTrigger": "always", //["always","trigger","none"]
  "suggest.languageSourcePriority": 99,
  "suggest.numberSelect": false,
  "suggest.disableKind": false,
  "suggest.disableMenu": false,
  "suggest.snippetIndicator": "Ⓢ ",
  "suggest.maxCompleteItemCount": 50,
  "suggest.preferCompleteThanJumpPlaceholder": false,
  "suggest.fixInsertedWord": true,
  "suggest.localityBonus": true,
  "suggest.triggerAfterInsertEnter": false,
  "suggest.timeout": 2000,
  "suggest.minTriggerInputLength": 1,
  "suggest.triggerCompletionWait": 50,
  "suggest.echodocSupport": true,
  "suggest.acceptSuggestionOnCommitCharacter": false,
  "suggest.noselect": false,
  "suggest.keepCompleteopt": false,
  "suggest.lowPrioritySourceLimit": 90,
  "suggest.highPrioritySourceLimit": 90,
  "suggest.completionItemKindLabels": {
    "function": "\u0192",
    "method": "\uf09a",
    "variable": "\ue79b",
    "constant": "\uf8fe",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "file": "\uf723",
    "value": "\uf89f",
    "event": "\ufacd",
    "folder": "\uf114",
    "keyword": "\uf1de",
    "snippet": "\ue60b",
    "operator": "\u03a8",
    "reference": "\ufa46",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  // diagnostic
  "diagnostic.enable": true,
  "diagnostic.level": "hint", //["hint","information","warning","error"]
  "diagnostic.checkCurrentLine": true,
  "diagnostic.messageTarget": "float", //["echo","float"]
  "diagnostic.joinMessageLines": true,
  "diagnostic.refreshOnInsertMode": true,
  "diagnostic.refreshAfterSave": true,
  "diagnostic.displayByAle": false,
  "diagnostic.virtualText": false,
  "diagnostic.virtualTextPrefix": " ",
  "diagnostic.virtualTextLines": 3,
  "diagnostic.virtualTextLineSeparator": " \\ ",
  "diagnostic.enableSign": true,
  "diagnostic.errorSign": "●",
  "diagnostic.warningSign": "●",
  "diagnostic.enableMessage": "always", //["always","jump","never"]
  "diagnostic.locationlist": true,
  "diagnostic.highlightOffset": 10000,
  "diagnostic.signOffset": 99999999,
  "diagnostic.infoSign": ">>",
  "diagnostic.hintSign": ">>",
  "diagnostic.maxWindowHeight": 15,
  // Signature
  "signature.enable": true,
  "signature.target": "float", // ["float","echo"]
  "signature.preferShownAbove": true,
  "signature.hideOnTextChange": false,
  "signature.maxWindowHeight": 8,
  "codeLens.enable": false,
  "workspace.ignoredFiletypes": ["markdown", "log", "txt", "help"],
  "list.indicator": ">",
  "list.maxHeight": 15,
  "list.signOffset": 900,
  "list.selectedSignText": "*",
  "list.autoResize": true,
  "list.limitLines": 30000,
  "list.maxPreviewHeight": 12,
  "list.previewHighlightGroup": "Search",
  "list.nextKeymap": "<C-j>",
  "list.previousKeymap": "<C-k>",
  "list.normalMappings": {},
  "list.insertMappings": {},
  "list.source.outline.ctagsFilestypes": [],
  // preference
  "coc.preferences.useQuickfixForLocations": false,
  "coc.preferences.extensionUpdateCheck": "daily", // ["daily","weekly","never"]
  "coc.preferences.snippetStatusText": "Ⓢ ",
  "coc.preferences.hoverTarget": "float", // ["preview","echo","float"]
  "coc.preferences.colorSupport": true,
  "coc.preferences.previewAutoClose": true,
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.formatOnSaveFiletypes": [],
  "coc.preferences.enableFloatHighlight": true,
  "coc.preferences.rootPatterns": [".vim", ".git", ".hg"],
  "coc.preferences.jumpCommand": "edit", // ["edit","split","vsplit","tabe","drop","tab drop"]
  "coc.preferences.messageLevel": "more", // ["more","warning","error"]
  "coc.preferences.bracketEnterImprove": true,
  "coc.preferences.formatOnType": true,
  "coc.preferences.snippets.enable": true,
  // snippets
  "snippets.loadFromExtensions": false,
  "snippets.ultisnips.enable": true,
  "snippets.snipmate.enable": false,
  "snippets.ultisnips.directories": ["~/.config/nvim/snippets"],
  "snippets.extends": {
    "cpp": ["c"],
    "javascriptreact": ["javascript"],
    "typescript": ["javascript"]
  },
  // yank
  "yank.highlight.enable": false,
  "yank.list.maxsize": 1000,
  "yank.enableCompletion": false,
  "yank.priority": 90,
  "yank.limit": 3,
  //css
  // Coc-list
  "session.directory": "~/.cache/nvim/sessions",
  "list.source.mru.ignoreGitIgnore": false,
  "list.source.grep.useLiteral": true,
  "list.source.grep.command": "rg",
  // coc-git
  "git.addedSign.hlGroup": "GutterAdd",
  "git.changedSign.hlGroup": "GutterChange",
  "git.removedSign.hlGroup": "GutterDelete",
  "git.topRemovedSign.hlGroup": "GutterDelete",
  "git.changeRemovedSign.hlGroup": "GutterChangeDelete",
  "coc.source.issues.filetypes": ["gitcommit"],
  // coc-gitignore
  "gitignore.enable": true,
  "gitignore.templates.patch": {},
  "gitignore.templates.replace": {},
  //coc-translator
  "translator.toLang": "zh",
  "translator.engines": ["bing", "ciba"],
  "translator.maxsize": 5000,
  "translator.window.maxWidth": 0,
  "translator.window.maxHeight": 0,
  // ts,js
  // python
  "python.autoComplete.addBrackets": true,
  "python.jediEnabled": true,
  "python.jediMemoryLimit": -1,
  "python.jediPath": "/usr/local/lib/python3.7/site-packages",
  "python.analysis.diagnosticPublishDelay": 10,
  "python.linting.enabled": true,
  "python.linting.lintOnSave": true,
  "python.linting.pylintArgs": ["--rcfile", "~/.pylintrc"],
  "python.linting.pylintEnabled": true,
  "python.linting.pylintUseMinimalCheckers": true,
  "python.pythonPath": "/usr/local/bin/python3",
  "python.sortImports.path": "/usr/local/bin/isort",
  "python.terminal.executeInFileDir": true,
  // Viml
  "vimlsp.debug": true,
  "vimlsp.trace.server": "verbose", // "enum": ["off", "messages", "verbose"],
  "vimlsp.diagnostic.enable": true,
  "vimlsp.indexes.runtimepath": true,
  "vimlsp.indexes.count": 5,
  "vimlsp.indexes.gap": 50,
  "vimlsp.indexes.projectRootPatterns": [".git", "autoload", "plugin"],
  "vimlsp.suggest.fromVimruntime": true,
  "vimlsp.suggest.fromRuntimepath": true,
  //language server
  "languageserver": {
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"],
      "ignoredRootPaths": ["~"]
    },
    "ccls": {
      "command": "ccls",
      "filetypes": ["c", "cpp", "objc", "objcpp"],
      "rootPatterns": [
        ".ccls",
        "compile_commands.json",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "initializationOptions": {
        "cache": {
          "directory": "/tmp/ccls"
        }
      }
    }
  },
  "git.enableGutters": true
}
